{
  "name": "zeromq-ng",
  "version": "5.0.0-beta.3",
  "description": "Next-generation ZeroMQ bindings for Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/rolftimmermans/zeromq-ng.git"
  },
  "dependencies": {
    "detect-libc": ">= 0.2.0",
    "node-addon-api": "rolftimmermans/node-addon-api#master",
    "prebuild-install": ">= 2.2.2"
  },
  "devDependencies": {
    "@types/node": ">= 8.0",
    "benchmark": ">= 0",
    "chai": ">= 4.1",
    "eslint": ">= 4.0",
    "mocha": ">= 4.0",
    "prebuild": ">= 6.2.1",
    "semver": ">= 0",
    "typescript": "^2.5.3",
    "weak": ">= 1.0"
  },
  "engines": {
    "node": ">= 8.6"
  },
  "scripts": {
    "install": "detect-libc prebuild-install || script/build.sh",
    "prebuild": "detect-libc prebuild --verbose --target 8.6.0 --strip --upload ${GH_TOKEN}",
    "test": "eslint lib && script/fmt.sh && script/build.sh --verbose && mocha && tsc --project test/unit",
    "bench": "script/build.sh --verbose && node --use_strict --harmony_async_iteration --expose-gc test/bench"
  },
  "keywords": [
    "zeromq",
    "zmq",
    "0mq",
    "Ã¸mq",
    "libzmq",
    "native",
    "binding",
    "addon"
  ],
  "license": "MIT"
}
