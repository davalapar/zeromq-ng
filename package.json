{
  "name": "zeromq",
  "version": "5.0.0",
  "description": "ZeroMQ bindings for Node.js",
  "main": "lib/index",
  "gypfile": true,
  "dependencies": {
    "node-addon-api": "rolftimmermans/node-addon-api#master",
    "node-gyp": ">= 3.6.2"
  },
  "devDependencies": {
    "benchmark": ">= 0",
    "chai": ">= 4.1",
    "mocha": ">= 4.0",
    "semver": ">= 0",
    "should": ">= 0",
    "weak": ">= 1.0"
  },
  "engines": {
    "node": ">= 8.6"
  },
  "scripts": {
    "test": "node node_modules/node-gyp/bin/node-gyp.js rebuild && node node_modules/mocha/bin/mocha",
    "bench": "node node_modules/node-gyp/bin/node-gyp.js rebuild && node --use_strict --harmony_async_iteration --expose-gc test/bench/send-only.js",
    "fmt": "find src -path -prune -o \\( -name \"*.cc\" -or -name \"*.h\" \\) -exec clang-format -i -style=file {} \\;"
  },
  "keywords": [
    "zeromq",
    "zmq",
    "0mq",
    "Ã¸mq",
    "libzmq",
    "native",
    "binding",
    "addon"
  ],
  "license": "MIT"
}
